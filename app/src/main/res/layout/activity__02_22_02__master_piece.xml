<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="._02_22_02_MasterPieceActivity">

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="120dp"
        android:layout_height="145dp"
        android:layout_marginTop="135dp"
        app:layout_constraintEnd_toStartOf="@+id/iv2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pic1" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="120dp"
        android:layout_height="145dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv2"
        app:layout_constraintTop_toTopOf="@+id/iv1"
        app:srcCompat="@drawable/pic3" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="120dp"
        android:layout_height="145dp"
        app:layout_constraintEnd_toStartOf="@+id/iv3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv1"
        app:layout_constraintTop_toTopOf="@+id/iv1"
        app:srcCompat="@drawable/pic2" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="120dp"
        android:layout_height="145dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/iv1"
        app:layout_constraintTop_toBottomOf="@+id/iv1"
        app:srcCompat="@drawable/pic4" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="120dp"
        android:layout_height="145dp"
        app:layout_constraintStart_toStartOf="@+id/iv2"
        app:layout_constraintTop_toTopOf="@+id/iv4"
        app:srcCompat="@drawable/pic5" />

    <ImageView
        android:id="@+id/iv6"
        android:layout_width="120dp"
        android:layout_height="145dp"
        app:layout_constraintStart_toStartOf="@+id/iv3"
        app:layout_constraintTop_toTopOf="@+id/iv5"
        app:srcCompat="@drawable/pic6" />

    <ImageView
        android:id="@+id/iv7"
        android:layout_width="120dp"
        android:layout_height="145dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/iv4"
        app:layout_constraintTop_toBottomOf="@+id/iv4"
        app:srcCompat="@drawable/pic7" />

    <ImageView
        android:id="@+id/iv8"
        android:layout_width="120dp"
        android:layout_height="145dp"
        app:layout_constraintStart_toStartOf="@+id/iv5"
        app:layout_constraintTop_toTopOf="@+id/iv7"
        app:srcCompat="@drawable/pic8" />

    <ImageView
        android:id="@+id/iv9"
        android:layout_width="120dp"
        android:layout_height="145dp"
        app:layout_constraintStart_toStartOf="@+id/iv6"
        app:layout_constraintTop_toTopOf="@+id/iv8"
        app:srcCompat="@drawable/pic9" />

    <Button
        android:id="@+id/resultbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:onClick="goRating"
        android:text="점수 보기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Master Piece"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>